{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "biblio",
      "program": "app/lib/main.dart",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": ["--dart-define-from-file=app/.env"],
      "env": {
        "PATH": "/opt/homebrew/bin:/usr/local/bin:${env:PATH}",
        "LANG": "en_US.UTF-8",
        "LC_ALL": "en_US.UTF-8"
      }
    }
  ]
}
